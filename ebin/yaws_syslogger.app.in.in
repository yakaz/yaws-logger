%% -*-erlang-*-
%% vim:ft=erlang:

{application, @PACKAGE_NAME@, [
    {description,
      "Yaws module to provide extended logger"
    },
    {vsn, "@VERSION@"},
    {modules, [
        %% DO NOT EDIT: Modules list is generated by Makefile.am.
      ]},
    {registered, [
        yaws_syslogger_sup
      ]},
    {applications, [
        kernel,
        stdlib,
        syslogger
      ]},
    {mod, {yaws_syslogger_app, []}},
    {env, [
        %% Configuration default values.
        %% DO NOT EDIT: The final configuration is made in sys.config.
        {access_logformat, default},
        {syslog_loglevel, info},
        {syslog_facility, local3}
      ]}
  ]}.
